                <exec.args>-classpath %classpath com.devoteam.srit.xmlloader.gui.TesterGui</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir>F:\workspace\MTS\mts-ericsson\target\mts\bin</exec.workingdir>
            </properties>
        </action>
        <action>
